{
  "name": "trader-dev-en",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "echo \"Error: no test specified\" && exit 1",
    "tw:build": "tailwindcss build ./tailwind.css -o ./public/%npm_config_name%/dist/tailwind.css",
    "tw:prod": "postcss build ./tailwind.css -o ./public/%npm_config_name%/dist/_tailwind.css",
    "tw:watch": "onchange 'tailwind.config.js' ./public/%npm_config_name%/tailwind.css' -- npm run tw:build",
    "dev": "npm-run-all --parallel tw:build watch live vs-code --name=%npm_config_name%",
    "vs-code": "code %INIT_CWD%/public/%npm_config_name%/",
    "live": " live-server --open=public/%npm_config_name%/ --watch=public/%npm_config_name%/index.html",
    "watch": "nodemon --watch public/%npm_config_name%/_index.html edit.js %npm_config_name%",
    "check-env": "node -e console.log(process.argv[1])"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ansi-styles": "^3.2.1",
    "cli-spinner": "^0.2.8",
    "listdirs": "^3.1.1",
    "live-server": "^1.2.1",
    "node-key-sender": "^1.0.10",
    "node-storage": "0.0.7",
    "pretty": "^2.0.0",
    "prompt": "^1.0.0",
    "prompt-skeleton": "^1.0.2",
    "replace-in-file": "^6.2.0",
    "website-scraper": "^3.3.3"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "autoprefixer": "^10.2.4",
    "cheerio": "^1.0.0-rc.5",
    "cssnano": "^4.1.10",
    "jquery": "^3.5.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "onchange": "^7.1.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3"
  }
}
